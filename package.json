  "scripts": {
    "test": "cross-env NODE_ENV=test && npm run db:migrations && npm run db:seed && nyc mocha ./server/test/*.js --require @babel/register --exit",
    "start:dev": "set DEBUG=http && nodemon --exec babel-node ./server/index",
    "db:dropTable": "babel-node ./server/models/migrations dropTables",
    "db:createTable": "babel-node ./server/models/migrations createTables",
    "db:createUser": "babel-node ./server/models/migrations createUser",
    "db:seed": "npm run db:createUser",
    "db:migrations": "npm run db:dropTable && npm run db:createTable"
  },
